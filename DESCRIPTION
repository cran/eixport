Package: eixport
Title: Export Emissions to Atmospheric Models
Version: 0.3.0
Authors@R: c(
  person(given = "Sergio",
         family = "Ibarra-Espinosa",
         role = c("aut", "cre"),
         email = "sergio.ibarra@usp.br",
         comment = c(ORCID = "0000-0002-3162-1905")),
